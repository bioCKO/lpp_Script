                    "pbcoretools.task_options.scatter_alignments_reference_max_nchunks"
                ],
                "$schema": "http://json-schema.org/draft-04/schema#",
                "type": "object",
                "properties": {
                    "pbcoretools.task_options.scatter_alignments_reference_max_nchunks": {
